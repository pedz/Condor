<!-- app/views/shipped_files/show.xhtml.erb -->
<% @title = "Files with SHA1 equal to #{params[:sha1]}" %>
<h2><%= @title %></h2>
<table>
  <thead>
    <tr>
      <th>aix file</th>
      <th>image path</th>
      <th>lpp</th>
      <th>package</th>
      <th>service pack</th>
      <th>aix file sha1</th>
      <th>vrmf</th>
    </tr>
  </thead>
  <tfoot>
    <tr><td></td></tr>
  </tfoot>
  <tbody>
    <% @shipped_files.each do |shipped_file| %>
      <tr>
	<td><%= h shipped_file.aix_file %></td>
	<td><%= h shipped_file.image_path %></td>
	<td><%= h shipped_file.lpp %></td>
	<td><%= h shipped_file.package %></td>
	<td><%= h shipped_file.service_pack %></td>
	<td><%= h shipped_file.aix_file_sha1 %></td>
	<td><%= h shipped_file.vrmf %></td>
      </tr>
  <%- end -%>
  </tbody>
</table>
