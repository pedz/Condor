<h1>Editing fileset_ptf_map</h1>

<%= error_messages_for :fileset_ptf_map %>

<% form_for(@fileset_ptf_map) do |f| %>
  <p>
    <b>Fileset</b><br />
    <%= f.text_field :fileset_id %>
  </p>

  <p>
    <b>Ptf</b><br />
    <%= f.text_field :ptf_id %>
