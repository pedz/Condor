<%- @title = "Index of Image Path Package Maps"
<h1>Listing image_path_package_maps</h1>

<table>
  <tr>
    <th>Image path</th>
    <th>Package</th>
  </tr>

<% for image_path_package_map in @image_path_package_maps %>
  <tr>
    <td><%=h image_path_package_map.image_path_id %></td>
    <td><%=h image_path_package_map.package_id %></td>
    <td><%= link_to 'Show', image_path_package_map %></td>
    <td><%= link_to 'Edit', edit_image_path_package_map_path(image_path_package_map) %></td>
    <td><%= link_to 'Destroy', image_path_package_map, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New image_path_package_map', new_image_path_package_map_path %>
