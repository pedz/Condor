<!-- app/views/aix_files/index.html.erb -->
<%- @title = "Index of AIX Files"
<h1>Listing aix_files</h1>

<table>
  <tr>
    <th>Path</th>
  </tr>

<% for aix_file in @aix_files %>
  <tr>
    <td><%=h aix_file.path %></td>
    <td><%= link_to 'Show', aix_file %></td>
    <td><%= link_to 'Edit', edit_aix_file_path(aix_file) %></td>
    <td><%= link_to 'Destroy', aix_file, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New aix_file', new_aix_file_path %>
